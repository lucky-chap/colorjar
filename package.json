    "file-saver": "^2.0.5",
    "next": "12.2.1",
    "next-auth": "^4.9.0",
    "prettier": "^2.7.1",
    "react": "18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "tinygradient": "^1.1.5"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/cz-commitlint": "^17.0.3",
