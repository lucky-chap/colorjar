    "eslint": "8.19.0",
    "eslint-config-next": "12.2.1",
    "eslint-config-sznm": "^1.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "next-sitemap": "^3.1.10",
    "postcss": "^8.4.14",
    "prisma": "^4.0.0",
    "tailwindcss": "^3.1.5",
    "typescript": "4.7.4"
